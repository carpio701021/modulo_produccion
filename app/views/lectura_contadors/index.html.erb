<p id="notice"><%= notice %></p>

<h1>Lectura Contadors</h1>

<table>
  <thead>
    <tr>
      <th>Direccion</th>
      <th>Numerodecontador</th>
      <th>Lectura</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lectura_contadors.each do |lectura_contador| %>
      <tr>
        <td><%= lectura_contador.Direccion %></td>
        <td><%= lectura_contador.NumeroDeContador %></td>
        <td><%= lectura_contador.Lectura %></td>
        <td><%= link_to 'Show', lectura_contador %></td>
        <td><%= link_to 'Edit', edit_lectura_contador_path(lectura_contador) %></td>
        <td><%= link_to 'Destroy', lectura_contador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lectura Contador', new_lectura_contador_path %>
